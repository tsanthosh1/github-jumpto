{
  "manifest_version": 3,
  "name": "Github JumpTo",
  "version": "1.0",
  "description": "Quick access to GitHub repositories from your browsing history. Search and navigate to repositories, pull requests, issues, and more with keyboard shortcuts.",
  "author": "tsanthosh",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["history", "storage", "commands", "tabs"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+K",
        "mac": "Command+Shift+K"
      },
      "description": "Open Github Explore"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
